<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="arjun.pandey.com.ioeoldquestion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/question"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="arjun.pandey.com.ioeoldquestion.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".CivilEngineering" android:label="BCE"/>
        <activity android:name=".Architecture" android:label="B.Arch"/>
        <activity android:name=".ComputerEngineering" android:label="BCT"/>
        <activity android:name=".ElectronicsEngineering" android:label="BEX"/>
        <activity android:name=".CSIT" android:label="Bsc.CSIT"/>
    </application>

</manifest>